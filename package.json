{
  "name": "fcc-voting",
  "version": "1.0.0",
  "decription": "Free Code Camp Header parser",
  "author": "Brice Colucci <brice.colucci@gmail.com>",
  "private": true,
  "scripts": {
    "start": "npm run build && ./node_modules/.bin/nf start",
    "lint": "./node_modules/.bin/eslint src/**/*.js test/**/*.js --debug",
    "build": "rm -rf lib && npm run lint && ./node_modules/.bin/babel src -d lib",
    "test": "./node_modules/.bin/_mocha --compilers js:babel-register"
  },
  "license": "MIT",
  "dependencies": {
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.0.1",
    "jslint": "^0.10.1",
    "mocha": "^2.5.3",
    "should": "^9.0.2",
    "sinon": "^1.17.4"
  },
  "babel": {
    "presets": [ "es2015" ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    }
  }
}
