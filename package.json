{
  "name": "fcc-voting",
  "version": "1.0.0",
  "decription": "Free Code Camp Header parser",
  "author": "Brice Colucci <brice.colucci@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/nf start",
    "lint": "./node_modules/.bin/eslint src/**/*.js test/**/*.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "./node_modules/.bin/_mocha --compilers js:babel-register",
    "build": "rm -rf lib && npm run lint && npm run flow && npm run test && ./node_modules/.bin/babel src -d lib"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.0.1",
    "eslint-plugin-flowtype": "^2.3.0",
    "flow-bin": "^0.28.0",
    "jslint": "^0.10.1",
    "mocha": "^2.5.3",
    "should": "^9.0.2"
  }
}
